  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yamitsushi/esbuild-plugin-output-reset/issues"
  },
  "homepage": "https://github.com/yamitsushi/esbuild-plugin-output-reset#readme",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "esbuild": "^0.17.5"
  }
}
